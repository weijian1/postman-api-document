{
  "name": "postman-api-doc",
  "version" : "1.0.1",
  "description": "generate API document by Postman",
  "author": "weijian1",
  "scripts": {
    "dev": "webpack-dev-server --inline --hot --env.dev",
    "dev:doc": "webpack-dev-server --inline --hot --env.dev --config webpack_document.config.js",
    "build": "rimraf dist && webpack -p --progress --hide-modules",
    "build:doc": "rimraf doc && webpack -p --progress --hide-modules --config webpack_document.config.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.8",
    "@fortawesome/free-brands-svg-icons": "^5.5.0",
    "@fortawesome/free-solid-svg-icons": "^5.5.0",
    "@fortawesome/vue-fontawesome": "^0.1.2",
    "axios": "^0.18.0",
    "element-ui": "^2.3.4",
    "normalize.css": "^8.0.0",
    "vue": "^2.5.16",
    "vue-highlightjs": "^1.3.3",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "engines": {
    "node": ">=6"
  },
  "devDependencies": {
    "autoprefixer": "^6.6.0",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.0",
    "babel-preset-vue-app": "^1.2.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^0.27.0",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.24.1",
    "postcss-loader": "^1.3.3",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.8",
    "vue-loader": "^13.3.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2"
  }
}
